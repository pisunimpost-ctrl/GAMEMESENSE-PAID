-- Project Delta Lebyss.xys V5 (Stable Edition)
getgenv().script_key = "BKGFKHSbjtLfFNtXUeQpyjBqydZQQSOq";
getgenv().messagebox = function() end;


local function AntiLag()
    local lighting = game:GetService("Lighting")
    lighting.GlobalShadows = false
    lighting.FogEnd = 9e9
    settings().Rendering.QualityLevel = 1
    
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("PostEffect") or v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Enabled = false
        end
    end
end

task.spawn(AntiLag)

-- [ОЧИСТКА ПАМЯТИ]
task.spawn(function()
    while task.wait(60) do
        collectgarbage("collect")
        setfpscap(30) 
    end
end)

local function SafeLoad()
    local url = "https://raw.githubusercontent.com/Kapclantyler/wfr/refs/heads/main/radianthub"
    
    local success, content = pcall(function()
        return game:HttpGet(url)
    end)

    if success and content then
        local func, err = loadstring(content)
        if func then
            
                local run, runErr = pcall(func)
              if not run then warn("Script Error: " .. tostring(runErr)) end
            end)
        else
            warn("Loadstring Error: " .. tostring(err))
        end
    else
        warn("HTTP Error: Не удалось загрузить хаб")
    end
end

SafeLoad()
